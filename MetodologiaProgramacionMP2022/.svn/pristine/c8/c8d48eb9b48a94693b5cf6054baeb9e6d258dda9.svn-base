package org.mp.tema04;

import java.util.StringTokenizer;

public class EjemploSencilloStringTokenizer {

	public static void main(String[] args) {
		StringTokenizer st = new StringTokenizer("Esta es una pruebecilla valiente");
		while(st.hasMoreTokens()) { //delimitador car√°cter blanco
			System.out.println(st.nextToken());
		}

	}

}
