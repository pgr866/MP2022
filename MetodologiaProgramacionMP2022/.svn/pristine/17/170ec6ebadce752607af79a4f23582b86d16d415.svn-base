package org.mp.tema02;

import java.util.Arrays;

public class OrdenacionRectangulos {
	public static void main(String[] args) {

		RectanguloComparable[] rectangulos = { new RectanguloComparable(3.4, 5.4),
				new RectanguloComparable(13.24, 55.4), new RectanguloComparable(7.4, 35.4),
				new RectanguloComparable(1.4, 25.4) };

		Arrays.sort(rectangulos);

		
		for (int i = 0; i < rectangulos.length; i++) {
			System.out.print(rectangulos[i] + " ");
			System.out.println();
		}
		 System.out.println();
		 System.out.println();
				
		for (RectanguloComparable rectangulo : rectangulos) {
			System.out.print(rectangulo + " ");
			System.out.println();
		}
	}
}
